<p class="mb-5">
    Found <%= pluralize(@publishers.count, 'publisher') %> when searching for: <%= params[:keywords] %>
</p>

<div class="columns is-multiline">
    <% @publishers.each do |p| %>
    <div class="column is-one-quarter">
        <div>
        <%= link_to "#{p.name}", p %>
        </div>
    </div>
    <% end %>
</div>